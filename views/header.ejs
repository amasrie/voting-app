<!DOCTYPE html>
<html lang="en" class="coming-soon">
    <head>
        <meta charset="utf-8">
        <title>The Poll Center</title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
        <meta name="apple-mobile-web-app-capable" content="yes">
        <meta name="apple-touch-fullscreen" content="yes">
        <meta name="author" content="Abdalah Masrie">

        <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400italic,600' rel='stylesheet' type='text/css'>
        <link type="text/css" href="/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet">
        <link type="text/css" href="/fonts/themify-icons/themify-icons.css" rel="stylesheet">               <!-- Themify Icons -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link type="text/css" href="/stylesheets/styles.css" rel="stylesheet">
        <link type="text/css" href="assets/plugins/codeprettifier/prettify.css" rel="stylesheet">                <!-- Code Prettifier -->
        <link type="text/css" href="assets/plugins/iCheck/skins/minimal/blue.css" rel="stylesheet">              <!-- iCheck -->

    </head>


    <body class="infobar-overlay sidebar-hideon-collpase sidebar-scroll animated-content">

        <header id="topnav" class="navbar navbar-default navbar-fixed-top" role="banner">

            <div class="logo-area">

                <a href="/">
                    <div class="row">
                        <div class="header-logo">
                            <img width="65" src="/images/vote_alt.png" class="logo-margins" width="200px" />
                        </div>
                        <div class="col-sm-12 header-title">
                            <h1 class="header"><b>The Poll Center</b></h1>
                        </div>
                    </div>

                </a>

            </div>
            <!-- logo-area -->

            <nav class="navbar" role="navigation" id="headernav">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle icon-header" data-toggle="collapse" data-target="#horizontal-navbar">
                        <i class="ti ti-menu"></i>
                    </button>
                </div>
                <div class="collapse navbar-collapse large-icons-nav" id="horizontal-navbar">
                    <ul class="nav navbar-nav smart-menu">
                        <li>
                            <a href="/"><h4>Find a Poll</h4></a>
                        </li>
                        <% if(typeof user.name == 'undefined' || !user.name){ %>
                            <li>
                                <a href="/signin"><h4>Sign in</h4></a>
                            </li>
                            <li>
                                <a href="/signup"><h4>Sign up</h4></a>
                            </li>
                        <% }else{ %>
                            <li>
                                <a href="/mypolls"><h4>My Polls</h4></a>
                            </li>
                            <li>
                                <a href="/signout"><h4>Logout</h4></a>
                            </li>
                        <% } %>
                    </ul>
                </div>
            </nav>
        </header>

        <div id="wrapper">
            <div id="layout-static">

